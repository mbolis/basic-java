package it.sorint.training.ex3;

import it.sorint.training.IOUtil;

public class Main {

    public static void main(String[] args) {
        System.out.print("Hello, what's your name? ");
        String input = IOUtil.readln();
        if (input == null || input.isEmpty()) {
            System.exit(1);
        }

        
    }
}
