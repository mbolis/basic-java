public class Main {

    public static void main(String[] args) {

        int x, y, z;
        boolean b;
        
        x = 12;
        y = 10.0;
        z = "15";

        b = z > x;
        
        final String greeting = "Hello";
        greeting = "Howdy";

        Counter counter = new Counter();
        // output counter's inner counter value.
        counter.increment();
        // output incremented value.
    }
}
